<service>
	<id>retrochat.exe</id>
	<name>RetroChat</name>
	<description>Node JS RetroChat server.</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>--harmony</argument>
	<argument>c:\NodeApps\RetroChat\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\NodeApps\RetroChat\index.js</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>RetroChat wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<serviceaccount>
		<domain>YLAKERTA</domain>
		<user>LocalSystem</user>
		<password></password>
	</serviceaccount>
	<workingdirectory>c:\NodeApps\RetroChat</workingdirectory>
</service>